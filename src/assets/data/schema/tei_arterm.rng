      <oneOrMore>
        <ref name="model.addrPart"/>
        <zeroOrMore>
          <ref name="model.global"/>
        </zeroOrMore>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.address" combine="interleave">
    <empty/>
  </define>
  <!-- doc:(alternative identifier) contains an alternative or former structured identifier used for a manuscript or other object, such as a former catalogue number. [11.4. The Manuscript Identifier] -->
  <define name="altIdentifier">
    <element name="altIdentifier">
      <ref name="attlist.altIdentifier"/>
      <ref name="model.placeNamePart_sequenceOptional"/>
      <optional>
        <ref name="institution"/>
