      </zeroOrMore>
      <optional>
        <choice>
          <group>
            <ref name="model.div1Like"/>
            <zeroOrMore>
              <choice>
                <ref name="model.frontPart"/>
                <ref name="model.div1Like"/>
                <ref name="model.global"/>
              </choice>
            </zeroOrMore>
          </group>
          <group>
            <ref name="model.divLike"/>
            <zeroOrMore>
              <choice>
                <ref name="model.frontPart"/>
                <ref name="model.divLike"/>
                <ref name="model.global"/>
              </choice>
            </zeroOrMore>
          </group>
        </choice>
      </optional>
      <optional>
        <ref name="model.divBottomPart"/>
        <zeroOrMore>
          <choice>
            <ref name="model.divBottomPart"/>
            <ref name="model.global"/>
          </choice>
