            <optional>
              <ref name="extent"/>
            </optional>
            <ref name="publicationStmt"/>
            <zeroOrMore>
              <ref name="seriesStmt"/>
            </zeroOrMore>
            <optional>
              <ref name="notesStmt"/>
            </optional>
          </group>
          <zeroOrMore>
            <ref name="sourceDesc"/>
