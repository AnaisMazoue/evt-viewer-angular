      <choice>
        <value>lettre</value>
        <value>postface</value>
        <value>lexique</value>
        <value>titre</value>
        <value>privilege</value>
        <value>eloge</value>
        <value>avis</value>
        <value>poeme</value>
        <value>note</value>
      </choice>
    </attribute>
