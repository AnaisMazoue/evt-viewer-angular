      <ref name="macro.specialPara"/>
    </element>
  </define>
  <define name="attlist.quote" combine="interleave">
    <ref name="tei_att.global.attributes"/>
    <ref name="tei_att.typed.attributes"/>
  </define>
  <!-- doc:(reference) defines a reference to another location, possibly modified by additional text or comment. [3.7. Simple Links and Cross-References 17.1. Links] -->
