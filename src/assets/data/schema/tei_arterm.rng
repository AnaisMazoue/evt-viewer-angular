                <ref name="model.div1Like"/>
                <zeroOrMore>
                  <choice>
                    <ref name="model.global"/>
                    <ref name="_DUMMY_model.divGenLike"/>
                  </choice>
                </zeroOrMore>
              </oneOrMore>
            </choice>
          </optional>
        </group>
      </choice>
      <zeroOrMore>
        <ref name="model.divBottom"/>
        <zeroOrMore>
          <ref name="model.global"/>
