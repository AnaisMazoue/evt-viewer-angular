    </element>
  </define>
  <define name="attlist.objectName" combine="interleave">
    <optional>
      <attribute name="calendar"/>
    </optional>
  </define>
  <!-- doc:(occupation) contains an informal description of a person's trade, profession or occupation. [16.2.2. The Participant Description] -->
  <define name="occupation">
    <element name="occupation">
      <ref name="attlist.occupation"/>
