  <define name="_DUMMY_model.segLike" combine="choice">
    <notAllowed/>
  </define>
  <define name="_DUMMY_model.sourceDescPart" combine="choice">
    <notAllowed/>
  </define>
  <define name="_DUMMY_model.stageLike" combine="choice">
    <notAllowed/>
  </define>
  <define name="acquisition" combine="choice">
    <notAllowed/>
  </define>
  <define name="additional" combine="choice">
    <notAllowed/>
  </define>
  <define name="analytic" combine="choice">
    <notAllowed/>
  </define>
  <define name="argument" combine="choice">
    <notAllowed/>
  </define>
  <define name="catRef" combine="choice">
    <notAllowed/>
  </define>
  <define name="classCode" combine="choice">
    <notAllowed/>
  </define>  
  <define name="extent" combine="choice">
    <notAllowed/>
  </define>
  <define name="group" combine="choice">
    <notAllowed/>
  </define>
  <define name="listOrg" combine="choice">
    <notAllowed/>
  </define>
  <define name="listRelation" combine="choice">
    <notAllowed/>
  </define>
  <define name="meeting" combine="choice">
    <notAllowed/>
  </define>
  <define name="msFrag" combine="choice">
    <notAllowed/>
  </define>
  <define name="msItem" combine="choice">
    <notAllowed/>
  </define>
  <define name="msItemStruct" combine="choice">
    <notAllowed/>
  </define>
  <define name="msName" combine="choice">
    <notAllowed/>
  </define>
  <define name="msPart" combine="choice">
    <notAllowed/>
  </define>
  <define name="namespace" combine="choice">
    <notAllowed/>
  </define>
  <define name="objectIdentifier" combine="choice">
    <notAllowed/>
  </define>
  <define name="origin" combine="choice">
    <notAllowed/>
  </define>
  <define name="pc" combine="choice">
    <notAllowed/>
  </define>
  <define name="physDesc" combine="choice">
    <notAllowed/>
  </define>
  <define name="precision" combine="choice">
    <notAllowed/>
  </define>
  <define name="provenance" combine="choice">
    <notAllowed/>
  </define>
  <define name="purpose" combine="choice">
    <notAllowed/>
  </define>
  <define name="q" combine="choice">
    <notAllowed/>
  </define>
  <define name="relation" combine="choice">
    <notAllowed/>
  </define>
  <define name="schemaSpec" combine="choice">
    <notAllowed/>
  </define>
  <define name="series" combine="choice">
    <notAllowed/>
  </define>
  <define name="seriesStmt" combine="choice">
